# Editor configuration, see https://editorconfig.org
root = true

quote = single
quote_type = single

# Unix-style newlines with a newline ending every file
[*]
end_of_line = lf
insert_final_newline = true
indent_style = space
indent_size = 2

# Matches multiple files with brace expansion notation
# Set default charset
[*.tsx]
charset = utf-8
